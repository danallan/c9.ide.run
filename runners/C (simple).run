{
    "script" : [
        "set -e",
        "if [ \"$debug\" == true ]; then ",
            "gcc -ggdb3 -O0 -std=c99 $file -o \"$file\".o",
            "chmod 755 $file.o",
            "node /home/ubuntu/bin/c9gdbshim.js \"$file.o\" $args",
        "else",
            "gcc -std=c99 $file -o $file.o",
            "chmod 755 \"$file.o\"",
            "\"$file.o\" $args",
        "fi"
    ],
    "info": "Running \"$file\"",
    
    "socketpath": "/home/ubuntu/.c9/gdbdebugger.socket",
    "retryCount": 1000,
    "retryInterval": 300,
    "debugger": "gdb",
    "$debugDefaultState": false,
    
    "env" : {},
    "selector": "^.*\\.c$"
}
